{
    "name": "cycle/orm-promise-mapper",
    "type": "library",
    "require": {
        "php": ">=8.0",
        "cycle/orm": "2.0.x-dev",
        "doctrine/instantiator": "^1.3.1",
        "laminas/laminas-hydrator": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "spiral/code-style": "^1.0",
        "vimeo/psalm": "^4.13",
        "spiral/tokenizer": "^2.8"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Cycle\\ORM\\PromiseMapper\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cycle\\ORM\\PromiseMapper\\Tests\\": "tests/PromiseMapper"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
